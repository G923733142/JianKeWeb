package com.JianKe.pojo;

import java.util.Set;

public class Menu_Category {

	private int menu_cid;
	private String cname;
	private String image;

	private Set<Menu> menus;

	public Menu_Category(int menu_cid) {
		super();
		this.menu_cid = menu_cid;
	}

	public Menu_Category() {
	}

	public int getMenu_cid() {
		return menu_cid;
	}

	public void setMenu_cid(int menu_cid) {
		this.menu_cid = menu_cid;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public Set<Menu> getMenus() {
		return menus;
	}

	public void setMenus(Set<Menu> menus) {
		this.menus = menus;
	}

	public String getImage() {
		return image;
	}

	public void setImage(String image) {
		this.image = image;
	}

}
